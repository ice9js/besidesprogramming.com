{
  "name": "besidesprogramming.com",
  "version": "1.0.0",
  "description": "Client application for besidesprogramming.com",
  "main": "index.js",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.6",
    "@fortawesome/free-brands-svg-icons": "^5.4.1",
    "@fortawesome/free-solid-svg-icons": "^5.4.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "babel-core": "^6.26.3",
    "babel-loader": "7",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.6",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "express": "^4.16.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "highlight.js": "^10.4.1",
    "html-webpack-plugin": "^3.2.0",
    "lodash": "^4.17.21",
    "memory-fs": "^0.4.1",
    "node-fetch": "^2.6.1",
    "node-sass": "^4.14.1",
    "node-sass-package-importer": "^5.3.0",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-use-gesture": "^6.0.1",
    "redux": "^4.0.1",
    "require-from-string": "^2.0.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "3.1.11"
  },
  "scripts": {
    "build-css": "run-p -s build-css:*",
    "build-css:debug": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js --include-path client --source-map \"public/styles-debug.css.map\" assets/stylesheets/styles.scss public/styles-debug.css",
    "build-css:style": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js --include-path client assets/stylesheets/styles.scss public/styles.min.css --output-style compressed",
    "build-client": "webpack --display errors-only",
    "build-client:dev": "webpack --watch --inline --progress",
    "build-server": "webpack --display errors-only --config webpack.config.node.js",
    "server": "npm run build-server && node build/server.js",
    "start": "cross-env-shell NODE_ENV=development \"npm run build-css && run-p -s build-client:dev server\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ice9js/besidesprogramming.com.git"
  },
  "author": "Kuba Birecki <kuba.birecki@besidesprogramming.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ice9js/besidesprogramming.com/issues"
  },
  "homepage": "https://besidesprogramming.com"
}
